<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root spacing="10.0" stylesheets="@../css/bedrijfScherm.css" type="VBox" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <children>
      <Button fx:id="terugBtn" mnemonicParsing="false" onAction="#terug" prefHeight="26.0" prefWidth="55.0" text="terug" />
      <HBox fx:id="hBoxBoven" prefHeight="100.0" prefWidth="200.0" />
      <HBox fx:id="hBoxBeneden" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="5.0">
               <children>
                  <Button fx:id="afkeurenBtn" mnemonicParsing="false" onAction="#afkeuren" prefHeight="37.0" prefWidth="89.0" text="Afkeuren">
                     <VBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="aanvaardenBtn" mnemonicParsing="false" onAction="#aanvaarden" prefHeight="37.0" prefWidth="89.0" text="Goedkeuren">
                     <VBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
</fx:root>
